@{
    if (this.ViewData.TryGetValue("ParentLayout", out var parentLayout))
    {
        this.Layout = (string)parentLayout;
    }
    else
    {
        this.Layout = "/Areas/Identity/Pages/_Layout.cshtml";
    }
}

<h2>Manage your account</h2>

<div>
    <h4>Change your account settings</h4>
    <hr />
    <div class="row">
        <div class="col-md-3">
            <partial name="_ManageNav" />
        </div>
        <div class="col-md-9">
            @(this.RenderBody())
        </div>
    </div>
</div>

@section Scripts {
    @(this.RenderSection("Scripts", required: false))
}
