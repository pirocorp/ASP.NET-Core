@* ReSharper disable Html.PathError *@
@model int

@{
    this.ViewData["Title"] = "Delete Part";
}

<h2>Are you sure you want to delete this part?</h2>
<div class="text-right">
    <a href="/parts/destroy/@(this.Model)" class="btn btn-danger">Confirm Delete</a>
    <a href="/parts/all" class="btn btn-default">Back</a>
</div>